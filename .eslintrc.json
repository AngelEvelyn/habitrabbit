{
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": ["error"],
    "import/extensions": "off",
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [".js", ".jsx"]
      }
    ],
    "func-names": "off",
    "no-case-declarations": "off",
    "react/prefer-stateless-function": "off"
  },
  "env": {
    "browser": true,
    "node": true
  }
}
